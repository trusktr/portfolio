        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/screen.css" />
        <link rel='stylesheet' type='text/css' href="https://famo.us/lib/famous/famous.css" />
        <style>
            /*
             * Items marked with a "js" comment are set dynamically with javascript, and generally
             * need to be adjusted depending on device specifics like resolution, pixel ratio, aspect
             * ratio, touch abilities, etc.
             */

            /* reset */
            body, html {
                padding: 0;
                margin: 0;
                height: 100%;
                width: 100%;
                position: relative;
                background-color: white;
            }
            body {
                font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
                /*transform: translateZ(1px);*/ /*antialiasing in firefox*/
                overflow: hidden;
            }
            * {
                outline: 1px solid transparent;/*antialiasing in firefox*/
            }
            input {
                border: none;
            }
            a {
                color: inherit;
            }

            /* specific */
            input {
                padding: 4px;
            }
            input:active {
                outline: none;
              }
            .passwordBox input, .passwordBox button {
                margin-bottom: 5px;
            }

            .font-color-1 {
                color: #cff09e;
            }
            .font-color-2 {
                color: #A8DBA8;
            }
            .font-color-3 {
                color: #79BD9A;
            }
            .font-color-4 {
                color: #3B8686;
            }
            .font-color-5 {
                color: #0B486B;
            }

            .bg-color-1 {
                background: #cff09e;
            }
            .bg-color-2 {
                background: #A8DBA8;
            }
            .bg-color-3 {
                background: #79BD9A;
            }
            .bg-color-4 {
                background: #3B8686;
            }
            .bg-color-5 {
                background: #0B486B;
            }

            .border-color-1 {
                border-color: #cff09e;
            }
            .border-color-2 {
                border-color: #A8DBA8;
            }
            .border-color-3 {
                border-color: #79BD9A;
            }
            .border-color-4 {
                border-color: #3B8686;
            }
            .border-color-5 {
                border-color: #0B486B;
            }
            .border-color-5-dark {
                border-color: #053652
            }

            button, input {
                -webkit-transition: border 250ms, -webkit-box-shadow 250ms;
                -moz-transition:    border 250ms, -moz-box-shadow 250ms;
                -ms-transition:     border 250ms, -ms-box-shadow 250ms;
                -o-transition:      border 250ms, -o-box-shadow 250ms;
                transition:         border 250ms, box-shadow 250ms;
            }

            button {
                position: relative;
                color: #0B486B!important;

                background: rgba(217,255,161,1);
                background: -moz-linear-gradient(top, rgba(217,255,161,1) 0%, rgba(186,214,144,1) 100%);
                background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(217,255,161,1)), color-stop(100%, rgba(186,214,144,1)));
                background: -webkit-linear-gradient(top, rgba(217,255,161,1) 0%, rgba(186,214,144,1) 100%);
                background: -o-linear-gradient(top, rgba(217,255,161,1) 0%, rgba(186,214,144,1) 100%);
                background: -ms-linear-gradient(top, rgba(217,255,161,1) 0%, rgba(186,214,144,1) 100%);
                background: linear-gradient(to bottom, rgba(217,255,161,1) 0%, rgba(186,214,144,1) 100%);
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d9ffa1', endColorstr='#bad690', GradientType=0 );

                border: 2px solid #053652;
            }
            button:active {
                background: rgba(186,214,144,1);
                background: -moz-linear-gradient(top, rgba(186,214,144,1) 0%, rgba(217,255,161,1) 100%);
                background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(186,214,144,1)), color-stop(100%, rgba(217,255,161,1)));
                background: -webkit-linear-gradient(top, rgba(186,214,144,1) 0%, rgba(217,255,161,1) 100%);
                background: -o-linear-gradient(top, rgba(186,214,144,1) 0%, rgba(217,255,161,1) 100%);
                background: -ms-linear-gradient(top, rgba(186,214,144,1) 0%, rgba(217,255,161,1) 100%);
                background: linear-gradient(to bottom, rgba(186,214,144,1) 0%, rgba(217,255,161,1) 100%);
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#bad690', endColorstr='#d9ffa1', GradientType=0 );
            }
            button:hover {
                -webkit-box-shadow: 0px 0px 6px 0px rgba(207,240,158,0.7);
                -moz-box-shadow: 0px 0px 6px 0px rgba(207,240,158,0.7);
                -ms-box-shadow: 0px 0px 6px 0px rgba(207,240,158,0.7);
                -o-box-shadow: 0px 0px 6px 0px rgba(207,240,158,0.7);
                box-shadow: 0px 0px 6px 0px rgba(207,240,158,0.7);
            }
            input:hover {
                -webkit-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37), 0px 0px 6px 0px rgba(207,240,158,0.7);
                -moz-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37), 0px 0px 6px 0px rgba(207,240,158,0.7);
                -ms-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37), 0px 0px 6px 0px rgba(207,240,158,0.7);
                -o-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37), 0px 0px 6px 0px rgba(207,240,158,0.7);
                box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37), 0px 0px 6px 0px rgba(207,240,158,0.7);
            }

            input {
                -webkit-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37);
                -moz-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37);
                -ms-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37);
                -o-box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37);
                box-shadow: inset 0px 0px 10px 0px rgba(0,0,0,0.37);
                border: 2px solid #053652;
            }
            input:focus {
                border: 2px solid #cff09e;
            }

            input:input-placeholder {
                color: #244f4f;
            }
            input::-webkit-input-placeholder {
                color: #244f4f;
            }
            input:-moz-placeholder { /* Firefox 18- */
                color: #244f4f;
            }
            input::-moz-placeholder {  /* Firefox 19+ */
                color: #244f4f;
            }
            input:-ms-input-placeholder {
                color: #244f4f;
            }
        </style>
    </head>
    <body>
        <script src='/party/jspm/traceur.src.js'></script>
        <script src='/party/jspm/es6-module-loader.src.js'></script>
        <script src='/party/jspm/system.src.js'></script>
        <script src='jspm_config.js'></script>
        <script type="module">
            System.paths['js/*'] = 'javascripts/*/main.js';
            System.import('js/passwordReveal').then(m => {}, error => {
                console.error(error.stack);
            });
        </script>
    </body>
</html>

